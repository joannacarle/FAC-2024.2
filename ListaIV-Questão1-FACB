#include <stdio.h>

int main() {
    int x, i, n;
    int multiplos =0;

    printf("Digite um numero inteiro: ");
    scanf("%d", &x);

    for(i=1; i<= 50; i++){
        printf("Digite o %d numero inteiro: ", i);
        scanf("%d", &n);

        if(n % x == 0){
        multiplos++;
    }
}
    
    printf("Total de multiplos de %d, e igual a %d", x, multiplos);

    
}
